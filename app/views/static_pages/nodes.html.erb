


<% provide(:title, 'NODES') %>

  <div class="row">  
  <label>Node IP</label>
  <%= form_for(@node) do |f| %>
    
    <%= render 'shared/error_messages' %>
      <%= f.label :ip %>
      <%= f.text_field :ip%>
 <div class="control-group">  
    <label class="control-label" for="fileInput">Select The Server Key</label>  
